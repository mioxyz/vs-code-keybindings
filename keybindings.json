        "command": "workbench.action.toggleActivityBarVisibility"
    }
    // , { "key": "ctrl+shift+alt+p", "command": "extension.markdown-pdf.pdf" }
    // use ctrl+[l/j] for navigating to sites of interest, if find widget is not open.
    ,
    {
        "key": "ctrl+j",
        "command": "workbench.action.navigateBack",
        "when": "editorFocus && !findWidgetVisible"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.navigateForward",
        "when": "editorFocus && !findWidgetVisible"
    }
    // if find widget is open, use ctrl+[l/j] for switching between find results
    ,
    {
        "key": "up",
        "command": "editor.action.previousMatchFindAction",
        "when": "findWidgetVisible"
    },
    {
        "key": "down",
        "command": "editor.action.nextMatchFindAction",
        "when": "findWidgetVisible"
    }
    // { "key": "ctrl+l", "command": "editor.action.nextMatchFindAction",     "when": "findWidgetVisible" },
    // { "key": "ctrl+j", "command": "editor.action.previousMatchFindAction", "when": "findWidgetVisible" },
    // use j/l keys for navigating find history
    ,
    {
        "key": "ctrl+j",
        "command": "history.showPrevious",
        "when": "findWidgetVisible && historyNavigationEnabled && historyNavigationWidget"
    },
    {
        "key": "ctrl+l",
        "command": "history.showNext",
        "when": "findWidgetVisible && historyNavigationEnabled && historyNavigationWidget"
    },
